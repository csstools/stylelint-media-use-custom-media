{
  "name": "stylelint-media-use-custom-media",
  "version": "1.0.0",
  "description": "Enforce usage of custom media queries in CSS",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "csstools/stylelint-media-use-custom-media",
  "homepage": "https://github.com/csstools/stylelint-media-use-custom-media#readme",
  "bugs": "https://github.com/csstools/stylelint-media-use-custom-media/issues",
  "main": "index.cjs.js",
  "module": "index.es.mjs",
  "files": [
    "index.cjs.js",
    "index.cjs.js.map",
    "index.es.mjs",
    "index.es.mjs.map"
  ],
  "scripts": {
    "prepublishOnly": "npm test",
    "pretest": "rollup -c .rollup.js --silent",
    "test": "echo 'Running tests...'; npm run test:js && npm run test:tape",
    "test:js": "eslint *.js lib/*.js --cache --ignore-path .gitignore --quiet",
    "test:tape": "tape ./.tape.js | tap-spec"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^5.6.0",
    "eslint-config-dev": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "stylelint": "^9.5.0",
    "stylelint-test-rule-tape": "^0.2.0",
    "tap-spec": "^5.0.0"
  },
  "peerDependencies": {
    "stylelint": "^9.5.0"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  },
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "use",
    "custom",
    "media",
    "custom-media",
    "queries",
    "query",
    "variables",
    "variable",
    "vars",
    "var"
