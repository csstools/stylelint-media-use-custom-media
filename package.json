{
  "name": "stylelint-media-use-custom-media",
  "version": "2.0.0",
  "description": "Enforce usage of custom media queries in CSS",
  "author": "Jonathan Neal <jonathantneal@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "csstools/stylelint-media-use-custom-media",
  "homepage": "https://github.com/csstools/stylelint-media-use-custom-media#readme",
  "bugs": "https://github.com/csstools/stylelint-media-use-custom-media/issues",
  "main": "index.js",
  "module": "index.mjs",
  "files": [
    "index.js",
    "index.js.map",
    "index.mjs",
    "index.mjs.map"
  ],
  "scripts": {
    "prepublishOnly": "npm test",
    "pretest": "rollup -c .rollup.js --silent",
    "test": "npm run test:js && npm run test:tape",
    "test:js": "eslint src/*.js src/lib/*.js --cache --ignore-path .gitignore --quiet",
    "test:tape": "tape ./.tape.js | tap-spec"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-dev": "^2.0.0",
    "pre-commit": "^1.2.2",
    "rollup": "^2.9.1",
    "rollup-plugin-babel": "^4.4.0",
    "stylelint": "^14.0.1",
    "stylelint-test-rule-tape": "^0.2.0",
    "tap-spec": "^5.0.0"
  },
  "peerDependencies": {
    "stylelint": "10 - 14"
  },
  "eslintConfig": {
    "extends": "dev",
    "parser": "babel-eslint"
  },
  "keywords": [
    "stylelint",
    "stylelint-plugin",
    "css",
    "use",
    "custom",
    "media",
    "custom-media",
    "queries",
    "query",
    "variables",
    "variable",
    "vars",
    "var"
  ]
}
